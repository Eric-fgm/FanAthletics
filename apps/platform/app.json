{
	"expo": {
		"name": "Fan Athletics",
		"slug": "fan-athletics",
		"version": "1.0.0",
		"scheme": "fan-athletics",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "fan.athletics"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "fan.athletics"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro",
			"output": "single"
		},
		"plugins": ["expo-router"],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "1c67d425-334a-41a2-8867-d50c8e70b428"
			}
		}
	}
}
